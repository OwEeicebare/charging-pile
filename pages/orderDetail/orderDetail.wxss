/* pages/orderDetail/orderDetail.wxss */
.ing-order-progress,.finish-order-progress{
  height: 60rpx;
  margin: 20rpx 40rpx;
  line-height: 58rpx;
  border-radius: 30rpx;
  display: flex;
  justify-content: space-between;
}
.really-progress{
  padding-left: 20rpx;
  height: 100%;
  border-radius: 30rpx;
}
.finish-order-progress{
  text-align: center;
}
.finish-order-progress > .really-progress{
  width: 100%;
}
.progress-number{
  padding-right: 20rpx;
}
.order-information{
  padding: 40rpx;
  font-weight: normal;
}
.order-title{
  font-weight: 600;
}
.order-information > view:not(.order-title), .finish-detail > view,.ing-detail > view{
  font-weight: normal;
  display: flex;
  justify-content: space-between;
}
.finish-ele{
  height: 80rpx;
  line-height: 78rpx;
  text-align: center;
  margin-top: 40rpx;
}